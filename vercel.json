{
  "rewrites": [
    {
      "source": "/(.*)\\.(png|jpg|jpeg|gif|ico|json|js|css|map|svg|ttf|woff|woff2)",
      "destination": "/$1.$2"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npx expo export -p web && node scripts/post-build.js",
  "outputDirectory": "dist",
  "framework": null,
  "installCommand": "npm install"
}