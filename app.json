{
  "expo": {
    "name": "Career Compass",
    "slug": "CareerCompass",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "careercompass",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#667eea"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.careercompass.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#667eea"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.careercompass.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "display": "standalone",
      "startUrl": "/",
      "shortName": "CareerCompass",
      "name": "Career Compass - 진로 탐험",
      "backgroundColor": "#F2F2F7",
      "themeColor": "#667eea",
      "orientation": "portrait",
      "lang": "ko"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}